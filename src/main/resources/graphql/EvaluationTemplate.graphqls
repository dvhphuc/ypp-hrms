type Question {
    id: Int!
    questionName: String!
    questionDescription: String!
    questionWeight: Float!
}

type CategoryDTO {
    categoryId: Int!
    categoryName: String!
    questions: [Question!]
}

type TemplateQuestionDTO {
    template: TemplateDTO!
    categories: [CategoryDTO!]
}

type FeedbackDTO {
    id: Int!
    giverId: Int!
    giverName: String!
    giverProfileImage: String
    feedbackContent: String!
    createdAt: String!
}